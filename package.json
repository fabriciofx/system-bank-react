{
  "name": "system-bank-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky",
    "deploy": "scp -r dist/* persephone:/var/www/system-bank-react"
  },
  "lint-staged": {
    "*.{ts,tsx,js,html,css}": [
      "npm run lint"
    ],
    "*.test.{ts,tsx,js}": [
      "npm test"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@tanstack/react-query": "^5.90.9",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.9.6",
    "sweetalert2": "^11.26.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.9",
    "axios-mock-adapter": "^2.1.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "npm-check-updates": "^19.1.2",
    "playwright": "^1.56.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9",
    "vitest-browser-react": "^2.0.2"
  }
}
